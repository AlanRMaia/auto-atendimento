{
  "devDependencies": {
    "cypress": "^13.2.0",
    "cypress-map": "^1.21.1",
    "cypress-xpath": "^2.0.1"
  },
  "scripts": {
    "homolog": "cypress open --config baseUrl=https://sitcarga-spa-homologacao.azurewebsites.net/#/ --env ENVIRONMENT=homolog",
    "test": "cypress open --config baseUrl=https://autoatendimentoteste/#/ --env ENVIRONMENT=test",
    "dev": "cypress open --config baseUrl=http://localhost:9000/#/ --env ENVIRONMENT=dev",
    "headless:todos": "npm run headless:homolog && npm run headless:test",
    "headless:homolog": "cypress run --headless=new --config baseUrl=https://sitcarga-spa-homologacao.azurewebsites.net/#/ --env ENVIRONMENT=homolog",
    "headless:dev": "cypress run --headless=new --config baseUrl=http://localhost:9000/#/ --env ENVIRONMENT=dev",
    "headless:test": "cypress run --headless=new  --config baseUrl=http://autoatendimentoteste/#/ --env ENVIRONMENT=test",
    "headless:test:spec": "cypress run --headless=new --config baseUrl=http://autoatendimentoteste/#/ --env ENVIRONMENT=test --spec"    
    
  },
  "dependencies": {
    "@faker-js/faker": "^8.0.2",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^4.0.2",
    "cypress-get-table": "^1.0.1",
    "faker-br": "^0.4.1",
    "nodemailer": "^6.9.4"
  },

  "chromeWebSecurity": false
}
